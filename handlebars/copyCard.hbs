<div class="pop-over-header">
  <span class="pop-header-title">Copy Card</span>
  <a class="pop-close-btn" href="#"></a>
</div>
<div class="pop-over-content">
  <form class="copy-card-form" method="post" action="/cards/copy">
    <label>Title</label>
    <textarea class="js-autofocus copy-card-text" name="name" style="margin-bottom: 12px;">{{card.title}}</textarea>
    <label>Copy toâ€¦</label>
    <div class="form-grid">
      <div class="button-link setting form-grid-child form-grid-child-threequarters">
        <span class="label">List</span>
        <span class="value js-list-value"></span>
        <label>List</label>
        <select class="js-select-list">
          {{#selectedLists lists list}}{{/selectedLists}}
        </select>
      </div>
      <div class="button-link setting form-grid-child form-grid-child-quarter">
        <span class="label">Position</span>
        <span class="value js-pos-value"></span>
        <label>Position</label>
        <select class="js-select-position">
            {{#selectedPositionPlusOne cards}}{{/selectedPositionPlusOne}}
        </select>
      </div>
    </div>
    <input class="primary wide js-commit-position" type="submit" value="Create Card">
  </form>
</div>
